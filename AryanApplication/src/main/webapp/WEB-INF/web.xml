 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveAryan</servlet-name>
  	<display-name>SaveAryan</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveAryan</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchDashBoard</servlet-name>
  	<display-name>SearchDashBoard</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SearchDashBoard</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveAryan</servlet-name>
  	<url-pattern>/SaveAryan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchDashBoard</servlet-name>
  	<url-pattern>/SearchDashBoard</url-pattern>
  </servlet-mapping>
</web-app>
